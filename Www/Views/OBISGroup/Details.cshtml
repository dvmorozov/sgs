@model web.Models.ViewOBISGroup

@{
	ViewBag.Title = "Details of the OBIS group";
	Layout = "~/Views/Shared/_LayoutModeling.cshtml";
}

<h2>Details of the OBIS group value</h2>

<p>
	@Html.ActionLink("Edit", "Edit", new { id=Model.ID }) |
	@Html.ActionLink("OBIS Groups", "Index")
</p>

<fieldset>
