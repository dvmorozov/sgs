		</tr>
		<tr>
			<td style="border: 0 hidden;">
				<div class="display-label">Desciption</div>
			</td>
			<td style="border: 0 hidden;">
				<div class="display-field">
					@Html.DisplayFor(model => model.Description)
				</div>
			</td>
		</tr>
		<tr>
			<td style="border: 0 hidden;">
				<div class="display-label">Standard</div>
			</td>
			<td style="border: 0 hidden;">
				<div class="display-field">
