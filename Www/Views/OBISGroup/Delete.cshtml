@model web.Models.ViewOBISGroup

@{
	ViewBag.Title = "Delete OBIS group value";
	Layout = "~/Views/Shared/_LayoutModeling.cshtml";
}

<h2>Delete OBIS group value</h2>

<p>
	@Html.ActionLink("OBIS Groups", "Index")
</p>

<h3>Are you sure you want to delete this OBIS group value?</h3>
